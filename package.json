{
  "name": "etalas-plop-monorepo",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "npm run dev --workspace=frontend",
    "build": "npm run build --workspace=plop-templates",
    "publish-plop": "npm publish --workspace=plop-templates",
    "setup": "npm install && npm link ./packages/plop-templates --workspace=frontend"
  }
}
